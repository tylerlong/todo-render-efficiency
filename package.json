    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "parcel": "^2.13.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sort-package-json": "^2.10.1",
